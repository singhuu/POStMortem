<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.postmortem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/launchericon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LevelSystems.SwipeLevelActivity"></activity>
        <!-- activity android:name=".MenuActivity"></activity -->
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LevelSystems.TypeLevel" />
        <activity android:name=".LevelSystems.TapLevel" />
        <activity android:name=".LevelSystems.TapLevelActivity" />
        <activity android:name=".LevelSystems.PickUpLevelActivity" />
        <activity android:name=".LevelSystems.TypeLevelActivity" />
        <activity android:name=".LevelSystems.SwipeLevel" />
        <activity android:name=".MenuSystems.MainMenuActivity" />
        <activity android:name=".MenuSystems.GameOverMenuActivity" />
        <activity android:name=".MenuSystems.UserSelectMenuActivity" />
        <activity android:name=".MenuSystems.OptionMenuActivity" />
        <activity android:name=".LevelSystems.PickUpLevel" />
        <activity android:name=".MenuSystems.StatsMenuActivity" />
        <receiver android:name=".notifications.NotificationUpdate" />
    </application>

</manifest>